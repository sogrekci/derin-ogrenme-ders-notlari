    "#passes #shots #fans\n",
    "ih_weight = [\n",
    "    [0.1, 0.2, -0.1], #hid[0]\n",
    "    [0.1, 0.1, 0.9], #hid[1]\n",
    "    [0.1, 0.4, 0.1] #hid[2]\n",
    "]\n",
    "\n",
    "#hid[0] #hid[1] #hid[2]\n",
    "hp_weight = [\n",
